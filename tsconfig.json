{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true, /* enable all strict type-checking options */
		"typeRoots": [
			"node_modules/@types"
		],
		"plugins": [
			{
				"name": "@typescript-nameof/nameof"
			},
			{
				"transform": "@typescript-nameof/nameof"
			}
		],
	}
}